export class Book {
  constructor (newTitle, newAuthor , newDescription , newPages , newCurrentPage , newRead){ 
    this.title = newTitle;
    this.author = newAuthor;
    this.description = newDescription;
    this.pages = newPages;
    this.currentPage = newCurrentPage;
    this.read = newRead;

    }
}

